@model BasicItem

@section subheader
{
    <h1>
        @Html.DisplayBackButton(Url.Action(nameof(DataDefinitionController.Index)))
        Data Definition Details
    </h1>
}
<div class="card">
    <div class="card-header">
        Item Detail
    </div>
    <div class="card-body">
        @Html.DisplayForModel()
    </div>
    <div class="card-footer">
        @Html.DisplayEditButton(Model.EditUrlAction(Model))
        @Html.DisplayDeleteButton(Model.DeleteUrlAction(Model))
    </div>
</div>
